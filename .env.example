# ============================================================
# KIOSBANK API CONFIGURATION
# ============================================================
# Copy this file to .env and fill in your actual credentials

# KIOSBANK API Credentials
KIOSBANK_API_URL=https://development.kiosbank.com:4432/auth/Sign-On
KIOSBANK_API_USERNAME=your_username_here
KIOSBANK_API_PASSWORD=your_password_here

# KIOSBANK Payload Configuration
KIOSBANK_MITRA=DJI
KIOSBANK_ACCOUNT_ID=your_account_id_here
KIOSBANK_MERCHANT_ID=your_merchant_id_here
KIOSBANK_MERCHANT_NAME=your_merchant_name_here
KIOSBANK_COUNTER_ID=1

# ============================================================
# VPS GATEWAY CONFIGURATION
# ============================================================
# VPS details for SSH tunnel and proxy

VPS_IP=your_vps_ip_here
VPS_USER=your_vps_username_here
VPS_SSH_PORT=22

# ============================================================
# PROXY CONFIGURATION
# ============================================================
# SOCKS5 proxy settings

USE_PROXY=True
PROXY_HOST=127.0.0.1
PROXY_PORT=1080

# ============================================================
# IP WHITELIST CONFIGURATION
# ============================================================
# Registered IP address in KIOSBANK dashboard

REGISTERED_IP=your_registered_ip_here
